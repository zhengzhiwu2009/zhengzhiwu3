		<jaxws:endpoint id="monitor" implementor="com.monitor.MonitorImpl" address="/monitor" />
		监控webservice配置 -->
	<!-- Spring 注解加载	-->
	<context:component-scan base-package="com.whaty" />
	<!-- whaty aop cacheable 需要的aspectj -->
	<aop:aspectj-autoproxy proxy-target-class="true" />
	<aop:config proxy-target-class="true" />

	<!-- WhatyCache基础缓存服务 -->
	<bean id="cacheService"
		class="com.whaty.framework.cache.core.service.impl.EhCacheService"
		init-method="init">
		<property name="cacheNamePrefix" value="XMHZPH_" />
		<!-- <property name="ehCacheConfig" value="ehcache.xml" /> -->
		<property name="ehcacheManager">
			<ref bean="ehCacheManager" />
		</property>
		<property name="defaultCacheName"
			value="defaultCacheConfiguration" />
		<property name="nullMode" value="false" />
	</bean>


	<!-- WhatyCache注解缓存拦截器   factory-method="aspectOf"-->
	<bean id="cacheAspect"
		class="com.whaty.framework.cache.aopcache.aop.CacheAspect">
		<property name="cacheService" ref="cacheService" />
	</bean>

	<bean id="placeholderConfig"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="location">
			<value>classpath:init.properties</value>
		</property>
	</bean>
	<bean id="portalDataSource"
		class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName">
			<value>java:comp/env/jdbc/hzph</value>
		</property>
	</bean>
	<!-- 已将配置转移至tomcat jndi ，待删除
		<bean id="portalDataSource"
		class="com.mchange.v2.c3p0.ComboPooledDataSource"
