	<bean id="sessionFactory"
		class="org.springframework.orm.hibernate4.LocalSessionFactoryBean">
		<property name="dataSource">
			<ref bean="portalDataSource" />
		</property>
		<property name="mappingLocations">
			<!-- 自动扫描实体配置文件，可以加多个包 -->
			<list>
				<value>
					classpath:com/whaty/platform/entity/**/*.hbm.xml
				</value>
				<value>classpath:com/whaty/log/**/*.hbm.xml</value>
			</list>
		</property>
		<!-- 
			<property name="packagesToScan">
			<!- *注解方式，暂时无法使用，屏蔽，*自动扫描实体配置文件，可以加多个包 ->
			<list>
			<value>com.whaty.platform.entity.bean</value>
			<value>com.whaty.log</value>
			</list>
			</property>
		-->
		<!-- 取消实体映射配置方式，待删除 -->
		<property name="hibernateProperties">
			<props>
				<prop key="hibernate.dialect">
					${hibernate.dialect}
				</prop>
				<prop key="hibernate.show_sql">
					${hibernate.show_sql}
				</prop>
				<prop key="hibernate.jdbc.fetch_size">
					${hibernate.jdbc.fetch_size}
				</prop>
				<prop key="hibernate.jdbc.batch_size">
					${hibernate.jdbc.batch_size}
				</prop>
			</props>
		</property>
	</bean>

