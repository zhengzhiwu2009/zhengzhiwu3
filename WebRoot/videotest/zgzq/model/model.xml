<?xml version="1.0" encoding="GB2312"?>
<!--模版配置文件,注意一般情况下应该用TplTools.exe而不是手工修改这个文件-->
<Model>
	<Template>6</Template>
	<ModelId>Model_MSN</ModelId>
	<!--模版的ID,给程序用的-->
	<ModelFriendlyName>MSN风格</ModelFriendlyName>
	<!--模版的名称,显示用的-->
	<Version>
		<!--模版版本-->
		<Major>1</Major>
		<!--主版本号-->
		<Minor>0</Minor>
		<!--附版本号-->
		<Revision>0812</Revision>
		<!--修改号-->
	</Version>
	<DescriptionForProgrammer>MSN风格模版，有多种配色方案，可动态切换，frame.htm无须修改</DescriptionForProgrammer>
	<!--模版特点描述,给开发者看的-->
	<Description>MSN风格模版，有多种配色方案</Description>
	<!--模版特点描述,给用户看的-->
	<ColorStyles>
		<!--配色方案数据-->
		<ColorStyle>
			<ColorStyleId>ColorStyle_Blue</ColorStyleId>
			<!--配色方案ID,名命是首字母大写-->
			<ColorStyleFriendlyName>蓝色</ColorStyleFriendlyName>
			<!--配色方案名称-->
			<Path>colorstyle_blue</Path>
			<!--配色方案目录,全小写-->
			<Default/>
			<!--有这个节点代表此配色方案是模版默认配色方案-->
			<BackColor>#DEE6F5</BackColor>
			<!--背景色,一般较浅-->
			<ForeColor>#213884</ForeColor>
			<!--前景色,一般较深,用于滚动条的阴影或文字-->
			<DisplayColor>#DEE6F5</DisplayColor>
			<!--显示色,如果想让菜单里显示的颜色和背景色不一样,就增加这个节点.如果没有这个节点,菜单取背景色显示-->
		</ColorStyle>
		<ColorStyle>
			<ColorStyleId>ColorStyle_Red</ColorStyleId>
			<ColorStyleFriendlyName>红色</ColorStyleFriendlyName>
			<Path>colorstyle_red</Path>
			<BackColor>#E7D7D6</BackColor>
			<ForeColor>#94554A</ForeColor>
		</ColorStyle>
		<ColorStyle>
			<ColorStyleId>ColorStyle_lv</ColorStyleId>
			<ColorStyleFriendlyName>绿色</ColorStyleFriendlyName>
			<Path>colorstyle_lv</Path>
			<BackColor>#E8F0E2</BackColor>
			<ForeColor>#426D31</ForeColor>
		</ColorStyle>
		<ColorStyle>
			<ColorStyleId>ColorStyle_huang</ColorStyleId>
			<ColorStyleFriendlyName>黄色</ColorStyleFriendlyName>
			<Path>colorstyle_huang</Path>
			<BackColor>#EAE4CC</BackColor>
			<ForeColor>#9C9629</ForeColor>
		</ColorStyle>
		<ColorStyle>
			<ColorStyleId>ColorStyle_mghong</ColorStyleId>
			<ColorStyleFriendlyName>玫瑰红</ColorStyleFriendlyName>
			<Path>colorstyle_mghong</Path>
			<BackColor>#EACCE6</BackColor>
			<ForeColor>#9C287B</ForeColor>
		</ColorStyle>
		<ColorStyle>
			<ColorStyleId>ColorStyle_lan</ColorStyleId>
			<ColorStyleFriendlyName>淡蓝色</ColorStyleFriendlyName>
			<Path>colorstyle_lan</Path>
			<BackColor>#E2F4F6</BackColor>
			<ForeColor>#397D8C</ForeColor>
		</ColorStyle>
		<ColorStyle>
			<ColorStyleId>ColorStyle_zi</ColorStyleId>
			<ColorStyleFriendlyName>紫色</ColorStyleFriendlyName>
			<Path>colorstyle_zi</Path>
			<BackColor>#EBE3F6</BackColor>
			<ForeColor>#6B4194</ForeColor>
		</ColorStyle>
	</ColorStyles>
	<Settings>
		<ModelVideoPath>__HTTPVIDEO__</ModelVideoPath>
		<!--模版默认视频路径.如果为__HTTPVIDEO__或空就代表HTTP视频-->
		<ModelColorStyleId>ColorStyle_Blue</ModelColorStyleId>
		<!--模版ColorStyle设置.允许值为__DEFAULT__,__RANDOM__,和存在的ColorStyleId-->
		<ModelIsShowColorStyleMenu>true</ModelIsShowColorStyleMenu>
		<!--是否显示配色方案菜单,全小写-->
		<ModelIsShowAudioOnlyMenu>true</ModelIsShowAudioOnlyMenu>
		<!--是否显示仅播放音频菜单,全小写.其实这个设置并没有起作用,完全看课程的配置-->
		<ModelInitLeftSize>210</ModelInitLeftSize>
		<!--初始的视频栏大小,默认是224-->
		<ModelInitIsShowRightDown>true</ModelInitIsShowRightDown>
		<!--初始是否显示右下方信息栏,全小写-->
		<ModelIsShowTrademark>false</ModelIsShowTrademark>
		<!--是否显示商标.这个只能在模版配,课程没有这个配置.全小写-->
		<ModelUserName></ModelUserName>
		<!--显示在信息栏的制作单位,可以改成客户的名称-->
		<ModelPhotoFile>photo.jpg</ModelPhotoFile>
		<!--模版的仅播放音频时显示的图片-->
	</Settings>
</Model>
